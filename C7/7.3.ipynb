   "source": [
    "import tkinter as tk\n",
    "\n",
    "cua_so = tk.Tk()\n",
    "cua_so.title(\"Bài 7.3 - Font chữ\")\n",
    "\n",
    "# Tạo nhãn với font chữ\n",
    "nhan = tk.Label(\n",
    "    cua_so,\n",
    "    text=\"Lập trình Tkinter\",\n",
    "    font=(\"Arial\", 16, \"bold\")\n",
    ")\n",
    "nhan.pack(pady=20)\n",
    "\n",
    "cua_so.mainloop()\n"
   ]
  }
