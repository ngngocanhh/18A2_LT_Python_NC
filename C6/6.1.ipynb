   "source": [
    "import threading\n",
    "\n",
    "def worker():\n",
    "    print(\"Tên luồng:\", threading.current_thread().name)\n",
    "\n",
    "threads = []\n",
    "\n",
    "for i in range(5):\n",
    "    t = threading.Thread(target=worker, name=f\"Thread-{i+1}\")\n",
    "    threads.append(t)\n",
    "    t.start()\n",
    "\n",
    "for t in threads:\n",
    "    t.join()\n"
   ]
