   "source": [
    "import threading\n",
    "import math\n",
    "\n",
    "def factorial(n):\n",
    "    print(f\"Giai thừa {n} =\", math.factorial(n))\n",
    "\n",
    "numbers = [5, 7, 10]\n",
    "\n",
    "threads = []\n",
    "for n in numbers:\n",
    "    t = threading.Thread(target=factorial, args=(n,))\n",
    "    threads.append(t)\n",
    "    t.start()\n",
    "\n",
    "for t in threads:\n",
    "    t.join()\n"
   ]
  }
